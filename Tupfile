include_rules

: src/libmonkey/*.o |> !ar |> libmonkey.a
: src/sds/*.o |> !ar |> libsds.a
: src/vec/src/*.o |> !ar |> libvec.a

LIBS = -lmonkey -lsds -lvec
: src/monkey_repl/*.o | libmonkey.a libsds.a libvec.a |> !ld |> monkey_repl.exe
: src/monkey_test/*.o | libmonkey.a libsds.a libvec.a |> !ld |> monkey_test.exe
